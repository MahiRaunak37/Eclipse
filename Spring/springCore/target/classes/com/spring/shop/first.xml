<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<!-- bean definitions here -->
	<bean id="product" class="com.products.Product">
		<property name="prodId">
			<list>
				<value>101</value>
				<value>102</value>
				<value>103</value>
			</list>
		</property>
		<property name="prodName">
			<list>
				<value>Chocolate</value>
				<value>IceCream</value>
				<value>Coca Cola</value>
			</list>
		</property>
		<property name="prodPrice">
			<list>
				<value>150.00</value>
				<value>200.00</value>
				<value>100.00</value>
			</list>
		</property>
	</bean>
	
	<bean id="order" class="com.products.OrderDetails">
		<property name="orderId" value="124001"></property>
		<property name="isOrderDelivered" value="true"></property>
	</bean>
</beans>